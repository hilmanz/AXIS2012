                <div class="facebookLike">
					<a href="#" onclick="shareFB('{$locale.nav.kenapaaxis}','{$basedomain}','{$basedomain}assets/content/slider/whyaxis/1.jpg','AXISgsm','{$locale.knapaaxis.panduan}');"><img width="63" src="{$basedomain}assets/images/icon/facebook_share_button.png"></a>
                </div><!-- end .facebookLike -->
				<div class="shareTwit">
