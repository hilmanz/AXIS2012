.videoright p.bar {display:block;padding:10px 15px 20px 15px;font:bold 16px Helvetica, sans-serif;color:#7A1878;width:145px;-webkit-border-radius:10px 10px 0 0;-moz-border-radius:10px 10px 0 0;border-radius:10px 10px 0 0;behavior: url(./PIE.htc);}
.listvideos {display:block;padding:0 5px 15px 5px;height:390px;}
.scrollvid {height:345px;overflow:hidden;display:block;overflow-y:scroll;margin:0 !important;padding:0;}
.listvideos a.vidmore{position:relative;top:25px;display:block;padding:10px;background:#fff;font:bold 12px/12px Helvetica, sans-serif;color:#7a1878;width:80px;-webkit-border-radius:10px;-moz-border-radius:10px;border-radius:10px;behavior: url(./PIE.htc);text-align:center;}
.itemvideo {display:block;padding:15px 10px;height:auto !important;cursor:pointer;border-bottom:solid 1px #7A1878;}
.ui-state-active, .ui-state-hover {background:#7A1878;}
.itemvideo a {display:block;width:230px !important;min-height:5px !important;}
#text .itemvideo a{width:auto !important;height:auto !important;margin:0 !important;padding:0 !important;float:none !important;}
.itemvideo img{display:block;width:auto;margin:0 5px 0 0;float:left;}
.itemviddetail {width:109px;float:left;}
.itemviddetail h2, .itemviddetail span.titlevid {font:bold 13px/16px Helvetica, sans-serif !important;color:#7A1878;}
.ui-state-active span.titlevid, .ui-state-hover span.titlevid {color:#FFE900;}
.ui-state-active span.metavid, .ui-state-hover span.metavid {color:#fff !important;}
.itemviddetail h2 a, .itemviddetail span.titlevid a{color:#7a1878;}
.itemviddetail p, .itemviddetail span.metavid{display:inline !important;float:left;font:bold 10px/14px Helvetica, sans-serif;color:#807f80;padding:0 !important;}
.vidleftdesc {height:48px;width:660px;}

.jspContainer{overflow: hidden;position: relative;}
.jspPane{position: absolute;}
.jspVerticalBar{position: absolute;top: 0;right: 0;width: 10px;height: 100%;background: red;}
.jspHorizontalBar{position: absolute;bottom: 0;left: 0;width: 100%;height: 16px;background: red;}
.jspVerticalBar *,.jspHorizontalBar *{margin: 0;padding: 0;}
.jspCap{display: none;}
.jspHorizontalBar .jspCap{float: left;}
.jspTrack{background: #fcfcfc;position: relative;}
.jspDrag{background: #79177C;position: relative;top: 0;left: 0;cursor: pointer;min-height:20px;}
.jspHorizontalBar .jspTrack,.jspHorizontalBar .jspDrag{float: left;height: 100%;}
.jspArrow{background: #50506d;text-indent: -20000px;display: block;cursor: pointer;}
.jspArrow.jspDisabled{cursor: default;background: #80808d;}
.jspVerticalBar .jspArrow{height: 16px;}
.jspHorizontalBar .jspArrow{width: 16px;float: left;height: 100%;}
.jspVerticalBar .jspArrow:focus{outline: none;}
.jspCorner{background: #eeeef4;float: left;height: 100%;}
.jspDragTop, .jspDragBottom {display:block;width:10px;height:10px;position:absolute;top:0;left:0;background:transparent url('../images/slideup.png') no-repeat 0 0;padding:0 0 10px 0;}
.jspDragBottom {background:transparent url('../images/slidedown.png') no-repeat 0 10px;top:auto;bottom:0;padding:10px 0 0 0;}
* html .jspCorner {margin: 0 -3px 0 0;}

/* new menu system */
#slide_menu_container {position: relative; width: 195px; height: 363px; overflow: hidden; font-family: Arial; font-size: 11px; background-color: transparent; left: 5px;opacity:0;filter:alpha(opacity=0);}
#slide_menu_container h1 {padding: 2px 0;margin: 0px;font:normal 1.3em/.7em Matiz; color: #67257a;}
#slide_menu_container p {line-height: 1.2em; margin: 0 0 10px 0;font: normal 1em verdana, tahoma, arial, sans-serif; color: #67257a;padding:0;}
#slide_menu_1 { width: 195px; height: auto; position: absolute; top:0; left:0;}
#slide_menu_2 { width: 195px; height: auto; position: absolute; top:0; left: 195px; display: none; }
#slide_menu_3 { width: 195px; height: auto; position: absolute; top:0; left: 195px; display: none; }
a.menuslide1{cursor: pointer;}
a.slideBacktoMain{cursor: pointer;}
ul.menustyle { list-style: none; margin: 0px; padding: 0px;}
ul.menustyle li { line-height: 18px; border-bottom: 1px solid #cbc7cb; cursor: pointer; background-color: #ffffff; }
ul.menustyle li a{ display: block; color: #333333; text-decoration: none; padding: 3px 10px;}
ul.menustyle li a:link{ color: #333333; text-decoration: none;}
ul.menustyle li a:hover{ color: #ffffff; text-decoration: none; background-color: #ffcb05;}
ul.menustyle li.arrow{ background: #ffffff url('../images/arrow.gif') no-repeat right;}
a.slideonce1 {color: #ffffff; text-decoration: none; cursor: pointer; background-color: #767676; padding: 2px 10px; position: absolute; left: 142px; top: 335px;}
a.slideonce1:link {color: #ffffff; text-decoration: none; cursor: pointer;}
a.slideonce1:hover {color: #ffffff; text-decoration: underline; cursor: pointer;}
a.slideonce2 {color: #ffffff; text-decoration: none; cursor: pointer; background-color: #767676; padding: 2px 10px; position: absolute; left: 142px; top: 335px;}
a.slideonce2:link {color: #ffffff; text-decoration: none; cursor: pointer;}
a.slideonce2:hover {color: #ffffff; text-decoration: underline; cursor: pointer;}
a.slidetohome {color: #ffffff; text-decoration: none; cursor: pointer; background-color: #d1d1d1; padding: 2px 10px; position: absolute; left: 76px; top: 335px;}
a.slidetohome:link {color: #ffffff; text-decoration: none; cursor: pointer;}
a.slidetohome:hover {color: #ffffff; text-decoration: underline; cursor: pointer; background-color: #ffcb05;}
a.slideonce1, a.slideonce2, a.slidetohome {top:325px !important;}
div.hoverscroll {position:relative;}
div.hoverscroll div.listcontainer {overflow:hidden;}
div.hoverscroll div.fixed-listcontainer {overflow: hidden;float: left;}
div.hoverscroll ul.list {list-style:none;margin:0;padding:0;}
div.hoverscroll ul.list li.item {display:block;padding:0;}
div.hoverscroll div.arrow {position:absolute;float:none;background-color:#ccc;background-position:center;background-repeat:no-repeat;opacity:0.8;filter:alpha(opacity=80);}
div.hoverscroll div.fixed-arrow {display: block;float: left;background-color: #67257A;background-position: center;background-repeat: no-repeat;}
div.hoverscroll div.fixed-arrow:hover {background-color:#747373;}
div.hoverscroll div.arrow.top,div.hoverscroll div.arrow.bottom,div.hoverscroll div.fixed-arrow.top,div.hoverscroll div.fixed-arrow.bottom {left:0px;width:100%;height:18px;cursor:pointer;}
div.hoverscroll div.arrow.top,div.hoverscroll div.fixed-arrow.top {top:0px;background-image:url('../images/arrow_up.gif');}
div.hoverscroll div.arrow.bottom,div.hoverscroll div.fixed-arrow.bottom {bottom:0px;background-image:url('../images/arrow_dn.gif');}
/* end new menu system */